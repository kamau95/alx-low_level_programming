#include "main.h"
/**
 * _strlen - prints length of a string
 * @s: string variable
 * @len: variable that holds length of a string
 * Return: length of string
 */
int _strlen(char *s)
{
        int len = 0;

while (s[len] != '\0')
{
        len++;

}
	 return (len);

}
